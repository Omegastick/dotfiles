Xft.dpi: 192
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*background:            #1d2021
*foreground:            #fbf1c7
*fading:                15
*fadeColor:             black
*cursorColor:           #fbf1c7
*pointerColorBackground:#1d2021
*pointerColorForeground:#fbf1c7

! special
*.foreground:   #fbf1c7
*.background:   #1d2021
*.cursorColor:  #fbf1c7

! black
*.color0:       #1d2021
*.color8:       #928374

! red
*.color1:       #cc241d
*.color9:       #fb4934

! green
*.color2:       #98971a
*.color10:      #b8bb26

! yellow
*.color3:       #d79921
*.color11:      #fabd2f

! blue
*.color4:       #458588
*.color12:      #83a598

! magenta
*.color5:       #b16286
*.color13:      #d3869b

! cyan
*.color6:       #689d6a
*.color14:      #8ec07c

! white
*.color7:       #a89984
*.color15:      #fbf1c7

! rofi
! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #1d2021, #928374, #928374
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #1d2021, #fbf1c7, #1d2021, #1d2021, #928374
rofi.color-active: #1d2021, #fb4934, #1d2021, #1d2021, #928374
rofi.color-urgent: #1d2021, #fbf1c7, #1d2021, #1d2021, #928374

rofi.separator-style: none
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Mononoki NF 24
!rofi.font: System San Francisco Display 18
rofi.bw: 2
rofi.columns: 1
rofi.padding: 32
rofi.fixed-num-lines: false
rofi.hide-scrollbar: true
rofi.width: 10
